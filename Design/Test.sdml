				<num n="num2" />
			</for>
		</add>
	</cache>

	<collect number id="list">
		<for array="range(int(number1))" iter="num2">
			<num n="num2" />
		</for>
	</collect>
	<!-- <compute int id="compute" exp="10" /> -->

	<!-- 10 - 5 + 5 + 1 + 5 + 1 + 5 = 22 -->
	<slots a="10.0">
		<num2 int="5" float="number2" />
		<num2 int="5" float="number2" />
		<num n="number1" />
	</slots>

	<add id="add">
		<num2 int="1" float="4.0" />
		<num n="int(#sum.result)" />
