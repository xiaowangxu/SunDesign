<refs>
</refs>
<inputs>
	<float name="time" default="2.5" />
	<int name="count" default="10" />
</inputs>
<type>
	<number count="2" />
</type>
<template>
	<add>
		<for array="range(count)" index="index" iter="iter">
			<num n="time + index" />
			<num n="count + iter" />
		</for>
	</add>

	<if test="count == 10">
		<num n="5" />
		<else>
			<num n="233" />
		</else>
	</if>
</template>